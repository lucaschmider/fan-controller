{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=postgres;Username=postgres;Password=postgres"
  },
  "Serial": {
    "SerialPort": "/dev/cu.usb289",
    "BaudRate": 9600,
    "LineSeparator": ";",
    "Pattern": "dutyCycle=(?<dutyCycle>\\d+),rpm=(?<speed>\\d+)"
  },
  "Business": {
    "SaveInterval": "00:10:00"
  }
}
